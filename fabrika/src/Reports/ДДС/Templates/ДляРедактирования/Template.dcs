<?xml version="1.0" encoding="UTF-8"?>
<DataCompositionSchema xmlns="http://v8.1c.ru/8.1/data-composition-system/schema" xmlns:dcscom="http://v8.1c.ru/8.1/data-composition-system/common" xmlns:dcscor="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xmlns:v8="http://v8.1c.ru/8.1/data/core" xmlns:v8ui="http://v8.1c.ru/8.1/data/ui" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<dataSource>
		<name>ИсточникДанных1</name>
		<dataSourceType>Local</dataSourceType>
	</dataSource>
	<dataSet xsi:type="DataSetQuery">
		<name>Запрос</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>Организация</dataPath>
			<field>Организация</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Получатель</dataPath>
			<field>Получатель</field>
			<role>
				<dcscom:ignoreNullValues>true</dcscom:ignoreNullValues>
			</role>
			<orderExpression>
				<dcscom:expression>ВЫБОР КОГДА ТИПЗНАЧЕНИЯ(Получатель)  = ТИП("Строка") ТОГДА 0 ИНАЧЕ ВЫБОР КОГДА ТИПЗНАЧЕНИЯ(Получатель)  = ТИП("Справочник.ПолучателиДДС") ТОГДА 1 ИНАЧЕ 2 КОНЕЦ КОНЕЦ </dcscom:expression>
				<dcscom:orderType>Asc</dcscom:orderType>
				<dcscom:autoOrder>false</dcscom:autoOrder>
			</orderExpression>
			<orderExpression>
				<dcscom:expression>Выбор Когда ТипЗначения(Получатель) = Тип("Строка") Тогда Подстрока(Получатель, 1, 1) Конец</dcscom:expression>
				<dcscom:orderType>Asc</dcscom:orderType>
				<dcscom:autoOrder>false</dcscom:autoOrder>
			</orderExpression>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ТипДвижения</dataPath>
			<field>ТипДвижения</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Тип движения</v8:content>
				</v8:item>
			</title>
			<role>
				<dcscom:ignoreNullValues>true</dcscom:ignoreNullValues>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СчетОрганизации</dataPath>
			<field>СчетОрганизации</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Счет организации</v8:content>
				</v8:item>
			</title>
			<presentationExpression>СчетОрганизации.Наименование+" "+СчетОрганизации.Валюта.Наименование</presentationExpression>
			<orderExpression>
				<dcscom:expression>СчетОрганизации.ПорядокПредставления</dcscom:expression>
				<dcscom:orderType>Asc</dcscom:orderType>
				<dcscom:autoOrder>false</dcscom:autoOrder>
			</orderExpression>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Сумма</dataPath>
			<field>Сумма</field>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:use>false</dcscor:use>
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ЧДЦ=2; ЧРД=,; ЧРГ=' '; ЧО=0</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Период</dataPath>
			<field>Период</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Видимость</dataPath>
			<field>Видимость</field>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ДанныеПроизводственногоКалендаря.Дата КАК Период
ПОМЕСТИТЬ ВТ_Период
ИЗ
	РегистрСведений.ДанныеПроизводственногоКалендаря КАК ДанныеПроизводственногоКалендаря
ГДЕ
	ДанныеПроизводственногоКалендаря.Дата МЕЖДУ &amp;Дата1 И &amp;Дата2

СГРУППИРОВАТЬ ПО
	ДанныеПроизводственногоКалендаря.Дата
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДДС.Организация КАК Организация,
	ДДС.СчетОрганизации КАК СчетОрганизации,
	ДДС.ТипДвижения КАК ТипДвижения,
	ДДС.Получатель КАК Получатель,
	ДДС.Период КАК Период,
	ДДС.Сумма КАК Сумма,
	ДДС.Сумма КАК СуммаПлан
ПОМЕСТИТЬ ВТ_Движения
ИЗ
	РегистрСведений.ДДС КАК ДДС
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.НастройкаПолейОтправки КАК НастройкаПолейОтправки
		ПО ДДС.Организация = НастройкаПолейОтправки.Организация
			И ДДС.ТипДвижения = НастройкаПолейОтправки.ТипДвижения
			И ДДС.Получатель = НастройкаПолейОтправки.Получатель
ГДЕ
	ДДС.Период МЕЖДУ &amp;Дата1 И &amp;Дата2

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Организации.Ссылка,
	БанковскиеСчета.Ссылка,
	ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.Поступление),
	ДДС.Организация,
	ВЫБОР
		КОГДА ДДС.ДатаПоступления = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА ДДС.Период
		ИНАЧЕ ДДС.ДатаПоступления
	КОНЕЦ,
	ВЫБОР
		КОГДА ДДС.Получатель = ЗНАЧЕНИЕ(Справочник.ПолучателиДДС.Конвертация)
			ТОГДА ДДС.СконвертированнаяСумма
		ИНАЧЕ ДДС.Сумма
	КОНЕЦ,
	NULL
ИЗ
	РегистрСведений.ДДС КАК ДДС
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.БанковскиеСчета КАК БанковскиеСчета
		ПО ДДС.СчетКонтрагента.НомерСчета = БанковскиеСчета.НомерСчета
			И ДДС.СчетКонтрагента.БИК = БанковскиеСчета.БИК
			И (ВЫБОР
				КОГДА ДДС.Организация.ВалютныйРассчёт
					ТОГДА ДДС.СчетКонтрагента.Валюта = БанковскиеСчета.Валюта
				ИНАЧЕ ИСТИНА
			КОНЕЦ)
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.НастройкаПолейОтправки КАК НастройкаПолейОтправки
		ПО ДДС.Организация = НастройкаПолейОтправки.Организация
			И ДДС.ТипДвижения = НастройкаПолейОтправки.ТипДвижения
			И ДДС.Получатель = НастройкаПолейОтправки.Получатель
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Организации КАК Организации
		ПО (ВЫБОР
				КОГДА ДДС.Получатель = ЗНАЧЕНИЕ(Справочник.ПолучателиДДС.Перевод)
						ИЛИ ДДС.Получатель = ЗНАЧЕНИЕ(Справочник.ПОлучателиДДС.Конвертация)
						ИЛИ ДДС.Получатель = ЗНАЧЕНИЕ(Справочник.ПОлучателиДДС.Депозит)
					ТОГДА ДДС.Организация.Ссылка = Организации.Ссылка
				ИНАЧЕ ДДС.Получатель.ИНН = Организации.ИНН
			КОНЕЦ)
ГДЕ
	НЕ Организации.Ссылка ЕСТЬ NULL
	И БанковскиеСчета.Владелец = Организации.Ссылка
	И ВЫБОР
			КОГДА ДДС.ДатаПоступления = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА ДДС.Период
			ИНАЧЕ ДДС.ДатаПоступления
		КОНЕЦ МЕЖДУ &amp;Дата1 И &amp;Дата2
	И ДДС.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.Списание)
	И ВЫБОР
			КОГДА &amp;ДеньВДень = ИСТИНА
				ТОГДА ДДС.ДеньВДень = &amp;ДеньВДень
			ИНАЧЕ ИСТИНА
		КОНЕЦ

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Организации.Ссылка,
	БанковскиеСчета.Ссылка,
	ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.Поступление),
	ДДС.Организация,
	ВЫБОР
		КОГДА ДДС.ДатаПоступления = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА ДДС.Период
		ИНАЧЕ ДДС.ДатаПоступления
	КОНЕЦ,
	NULL,
	ВЫБОР
		КОГДА ДДС.Получатель = ЗНАЧЕНИЕ(Справочник.ПолучателиДДС.Конвертация)
			ТОГДА ДДС.СконвертированнаяСумма
		ИНАЧЕ ДДС.Сумма
	КОНЕЦ
ИЗ
	РегистрСведений.ДДС КАК ДДС
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.БанковскиеСчета КАК БанковскиеСчета
		ПО ДДС.СчетКонтрагента.НомерСчета = БанковскиеСчета.НомерСчета
			И ДДС.СчетКонтрагента.БИК = БанковскиеСчета.БИК
			И (ВЫБОР
				КОГДА ДДС.Организация.ВалютныйРассчёт
					ТОГДА ДДС.СчетКонтрагента.Валюта = БанковскиеСчета.Валюта
				ИНАЧЕ ИСТИНА
			КОНЕЦ)
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.НастройкаПолейОтправки КАК НастройкаПолейОтправки
		ПО ДДС.Организация = НастройкаПолейОтправки.Организация
			И ДДС.ТипДвижения = НастройкаПолейОтправки.ТипДвижения
			И ДДС.Получатель = НастройкаПолейОтправки.Получатель
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Организации КАК Организации
		ПО (ВЫБОР
				КОГДА ДДС.Получатель = ЗНАЧЕНИЕ(Справочник.ПолучателиДДС.Перевод)
						ИЛИ ДДС.Получатель = ЗНАЧЕНИЕ(Справочник.ПолучателиДДС.Конвертация)
						ИЛИ ДДС.Получатель = ЗНАЧЕНИЕ(Справочник.ПолучателиДДС.Депозит)
					ТОГДА ДДС.Организация.Ссылка = Организации.Ссылка
				ИНАЧЕ ДДС.Получатель.ИНН = Организации.ИНН
			КОНЕЦ)
ГДЕ
	НЕ Организации.Ссылка ЕСТЬ NULL
	И БанковскиеСчета.Владелец = Организации.Ссылка
	И ВЫБОР
			КОГДА ДДС.ДатаПоступления = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА ДДС.Период
			ИНАЧЕ ДДС.ДатаПоступления
		КОНЕЦ МЕЖДУ &amp;Дата1 И &amp;Дата2
	И ДДС.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.Списание)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВТ_Движения.Организация КАК Организация,
	ВТ_Движения.СчетОрганизации КАК СчетОрганизации,
	ВТ_Движения.ТипДвижения КАК ТипДвижения,
	ВТ_Движения.Получатель КАК Получатель,
	ВТ_Движения.Период КАК Период,
	ВТ_Движения.Сумма КАК Сумма,
	ВТ_Движения.СуммаПлан КАК СуммаПлан
ПОМЕСТИТЬ ВТ_Движения_Остаток
ИЗ
	ВТ_Движения КАК ВТ_Движения

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_Движения.Организация,
	ВТ_Движения.СчетОрганизации,
	МАКСИМУМ(ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаКонец)),
	NULL,
	ВТ_Движения.Период,
	СУММА(ВЫБОР
			КОГДА ВТ_Движения.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.Списание)
				ТОГДА ВТ_Движения.Сумма * -1
			ИНАЧЕ ВТ_Движения.Сумма
		КОНЕЦ),
	СУММА(ВЫБОР
			КОГДА ВТ_Движения.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.Списание)
				ТОГДА ВТ_Движения.СуммаПлан * -1
			ИНАЧЕ ВТ_Движения.СуммаПлан
		КОНЕЦ)
ИЗ
	ВТ_Движения КАК ВТ_Движения

СГРУППИРОВАТЬ ПО
	ВТ_Движения.Организация,
	ВТ_Движения.СчетОрганизации,
	ВТ_Движения.Период

ИНДЕКСИРОВАТЬ ПО
	Организация,
	СчетОрганизации,
	Период
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Период.Период КАК Период,
	ВТ_Движения_Остаток.Организация КАК Организация,
	ВТ_Движения_Остаток.СчетОрганизации КАК СчетОрганизации,
	МАКСИМУМ(ВЫБОР
			КОГДА ВТ_Движения_Остаток.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаНачало)
				ТОГДА ВТ_Движения_Остаток.Период
		КОНЕЦ) КАК МасимальнаяДатаОстатка,
	МИНИМУМ(ВЫБОР
			КОГДА ВТ_Движения_Остаток.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаКонец)
				ТОГДА ВТ_Движения_Остаток.Период
		КОНЕЦ) КАК МаксимальнаяДатаОстаткаПоКонцу
ПОМЕСТИТЬ ВТ_ПоследнийОстаток
ИЗ
	ВТ_Период КАК ВТ_Период
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Движения_Остаток КАК ВТ_Движения_Остаток
		ПО (ВТ_Период.Период &gt;= ВТ_Движения_Остаток.Период
					И ВТ_Движения_Остаток.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаНачало)
				ИЛИ ВТ_Период.Период &gt; ВТ_Движения_Остаток.Период
					И ВТ_Движения_Остаток.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаКонец))
ГДЕ
	ВТ_Период.Период &lt;= &amp;ГраницаПлана

СГРУППИРОВАТЬ ПО
	ВТ_Период.Период,
	ВТ_Движения_Остаток.Организация,
	ВТ_Движения_Остаток.СчетОрганизации

ИНДЕКСИРОВАТЬ ПО
	Организация,
	СчетОрганизации,
	Период
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Движения_Остаток.Организация КАК Организация,
	ВТ_Движения_Остаток.СчетОрганизации КАК СчетОрганизации,
	ВТ_Движения_Остаток.ТипДвижения КАК ТипДвижения,
	ВТ_Движения_Остаток.Период КАК Период,
	ВТ_Движения_Остаток.Получатель КАК Получатель,
	ВТ_Движения_Остаток.Сумма КАК Сумма
ПОМЕСТИТЬ ВТ_Движения_Остаток_План
ИЗ
	ВТ_Движения_Остаток КАК ВТ_Движения_Остаток

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_ПоследнийОстаток.Организация,
	ВТ_ПоследнийОстаток.СчетОрганизации,
	ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаНачало),
	ВТ_ПоследнийОстаток.Период,
	НЕОПРЕДЕЛЕНО,
	СУММА(ВЫБОР
			КОГДА ВТ_Движения_Остаток.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.Списание)
				ТОГДА ВТ_Движения_Остаток.СуммаПлан * -1
			КОГДА ВТ_Движения_Остаток.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаКонец)
				ТОГДА 0
			ИНАЧЕ ВТ_Движения_Остаток.СуммаПлан
		КОНЕЦ)
ИЗ
	ВТ_ПоследнийОстаток КАК ВТ_ПоследнийОстаток
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_Движения_Остаток КАК ВТ_Движения_Остаток
		ПО ВТ_ПоследнийОстаток.Организация = ВТ_Движения_Остаток.Организация
			И (ВТ_ПоследнийОстаток.СчетОрганизации = ВТ_Движения_Остаток.СчетОрганизации
				ИЛИ ВТ_ПоследнийОстаток.СчетОрганизации = ВТ_Движения_Остаток.СчетОрганизации.Родитель
					И ВТ_Движения_Остаток.ТипДвижения &lt;&gt; ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаНачало))
			И (ВЫБОР
				КОГДА ВТ_ПоследнийОстаток.Период &lt;&gt; ЕСТЬNULL(ВТ_ПоследнийОстаток.МасимальнаяДатаОстатка, ВТ_ПоследнийОстаток.МаксимальнаяДатаОстаткаПоКонцу)
					ТОГДА ВТ_Движения_Остаток.Период &gt;= ЕСТЬNULL(ВТ_ПоследнийОстаток.МасимальнаяДатаОстатка, ВТ_ПоследнийОстаток.МаксимальнаяДатаОстаткаПоКонцу)
							И ВТ_Движения_Остаток.Период &lt; ВТ_ПоследнийОстаток.Период
			КОНЕЦ)

СГРУППИРОВАТЬ ПО
	ВТ_ПоследнийОстаток.Организация,
	ВТ_ПоследнийОстаток.СчетОрганизации,
	ВТ_ПоследнийОстаток.Период

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_ПоследнийОстаток.Организация,
	ВТ_ПоследнийОстаток.СчетОрганизации,
	ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаКонец),
	ВТ_ПоследнийОстаток.Период,
	НЕОПРЕДЕЛЕНО,
	СУММА(ВЫБОР
			КОГДА ВТ_Движения_Остаток.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.Списание)
				ТОГДА ВТ_Движения_Остаток.СуммаПлан * -1
			КОГДА ВТ_Движения_Остаток.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаКонец)
				ТОГДА 0
			ИНАЧЕ ВТ_Движения_Остаток.СуммаПлан
		КОНЕЦ)
ИЗ
	ВТ_ПоследнийОстаток КАК ВТ_ПоследнийОстаток
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_Движения_Остаток КАК ВТ_Движения_Остаток
		ПО ВТ_ПоследнийОстаток.Организация = ВТ_Движения_Остаток.Организация
			И (ВТ_ПоследнийОстаток.СчетОрганизации = ВТ_Движения_Остаток.СчетОрганизации
				ИЛИ ВТ_ПоследнийОстаток.СчетОрганизации = ВТ_Движения_Остаток.СчетОрганизации.Родитель
					И ВТ_Движения_Остаток.ТипДвижения &lt;&gt; ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаНачало))
			И (ВЫБОР
				КОГДА ВТ_ПоследнийОстаток.Период &lt;&gt; ЕСТЬNULL(ВТ_ПоследнийОстаток.МасимальнаяДатаОстатка, ВТ_ПоследнийОстаток.МаксимальнаяДатаОстаткаПоКонцу)
					ТОГДА ВТ_Движения_Остаток.Период &gt;= ЕСТЬNULL(ВТ_ПоследнийОстаток.МасимальнаяДатаОстатка, ВТ_ПоследнийОстаток.МаксимальнаяДатаОстаткаПоКонцу)
							И ВТ_Движения_Остаток.Период &lt; ВТ_ПоследнийОстаток.Период
			КОНЕЦ)

СГРУППИРОВАТЬ ПО
	ВТ_ПоследнийОстаток.Организация,
	ВТ_ПоследнийОстаток.СчетОрганизации,
	ВТ_ПоследнийОстаток.Период

ИНДЕКСИРОВАТЬ ПО
	Период,
	Организация,
	ТипДвижения,
	СчетОрганизации
;

