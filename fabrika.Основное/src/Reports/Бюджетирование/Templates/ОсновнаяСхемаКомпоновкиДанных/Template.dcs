			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	ДДС.Организация КАК Организация,
	ДДС.Период КАК День,
	ДДС.Сумма КАК Остаток,
	NULL КАК ТекущийОстаток,
	NULL КАК Прочие,
	ДДС.СчетОрганизации.Валюта КАК Валюта
ПОМЕСТИТЬ ВТ_Остатки
{ВЫБРАТЬ
	Организация.*,
	День,
	Остаток,
	ТекущийОстаток,
	Прочие}
ИЗ
	РегистрСведений.ДДС КАК ДДС
ГДЕ
	ДДС.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаНачало)
	И ДДС.Период = &amp;ДатаНачала
	И ДДС.СчетОрганизации.Валюта.Код В(&amp;КодыВалютОстатка)
	И ДДС.СчетОрганизации.Родитель = ЗНАЧЕНИЕ(Справочник.БанковскиеСчета.ПустаяСсылка)
{ГДЕ
	ДДС.Организация.*}

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ДДС.Организация,
	ДДС.Период,
	NULL,
	ДДС.Сумма,
	"Прочие",
	ДДС.СчетОрганизации.Валюта
ИЗ
	РегистрСведений.ДДС КАК ДДС
ГДЕ
	ДДС.ТипДвижения = ЗНАЧЕНИЕ(Перечисление.ТипДвиженияДДС.ОстатокНаНачало)
	И ДДС.Период = &amp;ТекущаяДата
	И ДДС.СчетОрганизации.Валюта.Код В(&amp;КодыВалютОстатка)
	И ДДС.СчетОрганизации.Родитель = ЗНАЧЕНИЕ(Справочник.БанковскиеСчета.ПустаяСсылка)
{ГДЕ
	ДДС.Организация.*}

ИНДЕКСИРОВАТЬ ПО
	День,
	Валюта
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Остатки.Организация КАК Организация,
	ВТ_Остатки.День КАК День,
	ВТ_Остатки.Остаток * ЕСТЬNULL(КурсыВалют.Курс, 1) КАК Остаток,
	ВТ_Остатки.ТекущийОстаток * ЕСТЬNULL(КурсыВалют.Курс, 1) КАК ТекущийОстаток,
	ВТ_Остатки.Прочие КАК Прочие
ИЗ
	ВТ_Остатки КАК ВТ_Остатки
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КурсыВалют КАК КурсыВалют
		ПО ВТ_Остатки.День = КурсыВалют.Период
			И ВТ_Остатки.Валюта = КурсыВалют.Валюта</query>
