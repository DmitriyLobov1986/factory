			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	ПланДДС.УИД КАК УИД,
	ПланДДС.День КАК День,
	ПланДДС.сп_Организация КАК ОрганизацияSQL,
	ПланДДС.сп_СтатьяРасхода КАК СтатьяРасхода,
	СУММА(ПланДДС.Приход) КАК Приход,
	СУММА(ПланДДС.Расход) КАК Расход,
	ПланДДС.Контрагент КАК Контрагент,
	сп_Организации.Ссылка КАК СвойКонтрагент,
	ВЫБОР
		КОГДА сп_Организации.Ссылка ЕСТЬ NULL
			ТОГДА 0
		ИНАЧЕ 1
	КОНЕЦ КАК Транзит,
	"Нет данных!!!" КАК НетДанных
ИЗ
	ВнешнийИсточникДанных.Бюджетирование.Таблица.ПланДДС КАК ПланДДС
		ЛЕВОЕ СОЕДИНЕНИЕ ВнешнийИсточникДанных.Бюджетирование.Таблица.сп_Организация КАК сп_Организации
		ПО ПланДДС.Контрагент = сп_Организации.ИНН
ГДЕ
	ПланДДС.День МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И &amp;РежимРедактирования = ЛОЖЬ

СГРУППИРОВАТЬ ПО
	ПланДДС.УИД,
	ПланДДС.День,
	ПланДДС.Контрагент,
	ПланДДС.сп_СтатьяРасхода,
	ПланДДС.сп_Организация,
	ВЫБОР
		КОГДА сп_Организации.Ссылка ЕСТЬ NULL
			ТОГДА 0
		ИНАЧЕ 1
	КОНЕЦ,
	сп_Организации.Ссылка

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	NULL,
	ВЫБОР
		КОГДА сп_Организации.Ссылка ЕСТЬ NULL
			ТОГДА ПланДДС.День
		ИНАЧЕ ДОБАВИТЬКДАТЕ(ПланДДС.День, ДЕНЬ, ЕСТЬNULL(ПланДДС.Приход, 0))
	КОНЕЦ,
	сп_Организации.Ссылка,
	ПланДДС.сп_СтатьяРасхода,
	СУММА(ПланДДС.Расход),
	NULL,
	ПРЕДСТАВЛЕНИЕ(ПланДДС.сп_Организация),
	ПланДДС.сп_Организация,
	2,
	"Нет данных!!!"
ИЗ
	ВнешнийИсточникДанных.Бюджетирование.Таблица.ПланДДС КАК ПланДДС
		ЛЕВОЕ СОЕДИНЕНИЕ ВнешнийИсточникДанных.Бюджетирование.Таблица.сп_Организация КАК сп_Организации
		ПО ПланДДС.Контрагент = сп_Организации.ИНН
ГДЕ
	ВЫБОР
			КОГДА сп_Организации.Ссылка ЕСТЬ NULL
				ТОГДА ПланДДС.День МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
			ИНАЧЕ ДОБАВИТЬКДАТЕ(ПланДДС.День, ДЕНЬ, ЕСТЬNULL(ПланДДС.Приход, 0)) МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
		КОНЕЦ
	И &amp;РежимРедактирования = ЛОЖЬ
	И НЕ сп_Организации.Ссылка ЕСТЬ NULL

СГРУППИРОВАТЬ ПО
	ПРЕДСТАВЛЕНИЕ(ПланДДС.сп_Организация),
	ПланДДС.сп_СтатьяРасхода,
	сп_Организации.Ссылка,
	ВЫБОР
		КОГДА сп_Организации.Ссылка ЕСТЬ NULL
			ТОГДА ПланДДС.День
		ИНАЧЕ ДОБАВИТЬКДАТЕ(ПланДДС.День, ДЕНЬ, ЕСТЬNULL(ПланДДС.Приход, 0))
	КОНЕЦ,
	ПланДДС.сп_Организация</query>
